{
  "expo": {
    "name": "CyberSaathi",
    "slug": "cybersaathi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "cybersaathi",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "description": "Your AI-powered cybersecurity companion. Stay safe, stay secure.",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cencops.cybersaathi"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0f172a"
      },


      "usesCleartextTraffic": true,
      "networkSecurityConfig": "network_security_config.xml",
      "edgeToEdgeEnabled": true,
      "package": "com.cencops.cybersaathi",
      "softwareKeyboardLayoutMode": "resize",
      "permissions": ["RECORD_AUDIO"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0f172a"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "aa8937af-acee-436f-98dc-dca3f6b9c1a7"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/aa8937af-acee-436f-98dc-dca3f6b9c1a7"
    }
  }
}
